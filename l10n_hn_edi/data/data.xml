<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">
    <!-- Sequences used for all Document types with Location and terminal 1 -->
    <record id="sequence_electronic_invoice_0010001" model="ir.sequence">
        <field name="name">FE 00000101</field>
        <field name="code">FE00000101</field>
        <field name="implementation">no_gap</field>
        <field eval="1" name="number_next"/>
        <field eval="1" name="number_increment"/>
        <field eval="False" name="use_date_range"/>
        <field eval="1" name="company_id"/>
        <field name="padding">8</field>
        <!-- Matriz - Sucursal -->
        <!-- 000    - 001 -->
        <field name="prefix">000-001-</field>
    </record>

